                    "data": 1,
                    "type": "INTEGER"
                },
                {
                    "name": "left_limit",
                    "data": 0.0,
                    "type": "DECIMAL"
                },
                {
                    "name": "right_limit",
                    "data": 100.0,
                    "type": "DECIMAL"
                }
            ],
            "outputs": [
                {
